            try {
                if (new Date(dateFrom.getTime() - (dateFrom.getTimezoneOffset() * 60000)).toISOString().substring(0, 10) == new Date(dateTo.getTime() - (dateTo.getTimezoneOffset() * 60000)).toISOString().substring(0, 10)) {
